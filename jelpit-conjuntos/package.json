{
  "name": "@jc/jelpit-conjuntos",
  "version": "0.0.0",
  "license": "MIT",
  "workspaces": [
    "/projects/apps/*",
    "/projects/lib/*"
  ],
  "scripts": {
    "shell:start:dev": "npm run start:dev    --workspace=shell",
    "shell:start:stage": "npm run start:stage  --workspace=shell",
    "shell:start:pre": "npm run start:pre  --workspace=shell",
    "shell:start:prod": "npm run start:prod  --workspace=shell",

    "shell:build:dev": "npm run build:dev   --workspace=shell",
    "shell:build:stage": "npm run build:stage --workspace=shell",
    "shell:build:pre": "npm run build:pre   --workspace=shell",
    "shell:build:prod": "npm run build:prod  --workspace=shell",

    "cuotas:start:dev": "ng serve --configuration dev   --project=@apps/cuotas --port 4001",
    "cuotas:start:stage": "ng serve --configuration stage --project=@apps/cuotas --port 4001",
    "cuotas:start:pre": "ng serve --configuration pre   --project=@apps/cuotas --port 4001",
    "cuotas:start:prod": "ng serve --configuration prod  --project=@apps/cuotas --port 4001",

    "cuotas:build:dev": "ng build --configuration dev   --project=@apps/cuotas",
    "cuotas:build:stage": "ng build --configuration stage --project=@apps/cuotas",
    "cuotas:build:pre": "ng build --configuration pre   --project=@apps/cuotas",
    "cuotas:build:prod": "ng build --configuration prod  --project=@apps/cuotas",

    "auth:start:dev": "ng serve --configuration dev   --project=@apps/auth --port 4002",
    "auth:start:stage": "ng serve --configuration stage --project=@apps/auth --port 4002",
    "auth:start:pre": "ng serve --configuration pre   --project=@apps/auth --port 4002",
    "auth:start:prod": "ng serve --configuration prod  --project=@apps/auth --port 4002",

    "auth:build:dev": "ng build --configuration dev   --project=@apps/auth",
    "auth:build:stage": "ng build --configuration stage --project=@apps/auth",
    "auth:build:pre": "ng build --configuration pre   --project=@apps/auth",
    "auth:build:prod": "ng build--configuration prod  --project=@apps/auth",

    "app4:start:dev": "ng serve --configuration dev   --project=@apps/app4 --port 4003",


    "build:single-spa:@apps/cuotas": "ng build @apps/cuotas --configuration production",
    "serve:single-spa:@apps/cuotas": "ng s --project @apps/cuotas --disable-host-check --port 4001 --live-reload false",
    "serve:single-spa:@apps/auth": "ng s --project @apps/auth --disable-host-check --port 4022 --live-reload false",
    "test": "jest --no-cache --coverage "
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.10",
