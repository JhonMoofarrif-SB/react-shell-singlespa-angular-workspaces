{
  "name": "@jc/jelpit-conjuntos",
  "version": "0.0.0",
  "license": "MIT",
  "workspaces": [
    "/projects/apps/*",
    "/projects/lib/*"
  ],
  "scripts": {
    "shell:start:dev": "npm run start:dev    --workspace=shell",
    "shell:start:stage": "npm run start:stage  --workspace=shell",
    "shell:start:pre": "npm run start:pre  --workspace=shell",
    "shell:start:prod": "npm run start:prod  --workspace=shell",

    "shell:build:dev": "npm run build:dev   --workspace=shell",
    "shell:build:stage": "npm run build:stage --workspace=shell",
    "shell:build:pre": "npm run build:pre   --workspace=shell",
    "shell:build:prod": "npm run build:prod  --workspace=shell",
    "cuotas:start:dev": "ng serve --configuration dev   --project=@apps/cuotas --port 4001",
    "cuotas:start:stage": "ng serve --configuration stage --project=@apps/cuotas --port 4001",
    "cuotas:start:pre": "ng serve --configuration pre   --project=@apps/cuotas --port 4001",
    "cuotas:start:prod": "ng serve --configuration prod  --project=@apps/cuotas --port 4001",
    "cuotas:build:dev": "ng build --configuration dev   --project=@apps/cuotas",
    "cuotas:build:stage": "ng build --configuration stage --project=@apps/cuotas",
    "cuotas:build:pre": "ng build --configuration pre   --project=@apps/cuotas",
    "cuotas:build:prod": "ng build --configuration prod  --project=@apps/cuotas",
    "herramientas:start:dev": "ng serve --configuration dev   --project=@apps/herramientas --port 4002",
    "herramientas:start:stage": "ng serve --configuration stage --project=@apps/herramientas --port 4002",
    "herramientas:start:pre": "ng serve --configuration pre   --project=@apps/herramientas --port 4002",
    "herramientas:start:prod": "ng serve --configuration prod  --project=@apps/herramientas --port 4002",
    "herramientas:build:dev": "ng build --configuration dev   --project=@apps/herramientas",
    "herramientas:build:stage": "ng build --configuration stage --project=@apps/herramientas",
    "herramientas:build:pre": "ng build --configuration pre   --project=@apps/herramientas",
    "herramientas:build:prod": "ng build --configuration prod  --project=@apps/herramientas",
    "build:single-spa:@apps/cuotas": "ng build @apps/cuotas --configuration production",
    "serve:single-spa:@apps/cuotas": "ng s --project @apps/cuotas --disable-host-check --port 4001 --live-reload false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.10",
    "@angular/common": "^14.2.10",
    "@angular/compiler": "^14.2.10",
    "@angular/core": "^14.2.10",
    "@angular/forms": "^14.2.10",
    "@angular/platform-browser": "^14.2.10",
    "@angular/platform-browser-dynamic": "^14.2.10",
    "@angular/router": "^14.2.10",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.9",
    "@types/systemjs": "^6.1.1",
    "primeicons": "^6.0.1",
    "primeng": "^14.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "6.4.3",
    "rxjs": "~7.5.7",
    "single-spa": ">=4.0.0",
    "single-spa-angular": "^7.1.0",
    "single-spa-react": "^5.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "14.0.1",
    "@angular-devkit/build-angular": "^14.2.9",
    "@angular/cli": "~14.2.9",
    "@angular/compiler-cli": "^14.2.10",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^2.2.0",
    "ts-config-single-spa": "^3.0.0",
    "tslib": "^2.4.1",
    "typescript": "^4.3.5",
    "vite": "^3.2.3",
    "vite-plugin-svgr": "^2.2.2",
    "vite-tsconfig-paths": "^3.5.2"
  }
}
