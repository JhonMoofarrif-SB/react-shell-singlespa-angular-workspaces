{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@apps/herramientas": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:resolver": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/apps/herramientas",
      "sourceRoot": "projects/apps/herramientas/src",
      "prefix": "herramientas",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/herramientas",
            "index": "projects/apps/herramientas/src/index.html",
            "main": "projects/apps/herramientas/src/main.ts",
            "polyfills": "projects/apps/herramientas/src/polyfills.ts",
            "tsConfig": "projects/apps/herramientas/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/apps/herramientas/src/favicon.ico",
              "projects/apps/herramientas/src/assets"
            ],
            "styles": [
              "projects/apps/herramientas/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "prod": {
              "fileReplacements": [
                {
                  "replace": "projects/apps/herramientas/src/environments/environment.ts",
                  "with": "projects/apps/herramientas/src/environments/environment.prod.ts"
                }
              ],
              "index": {
                "input": "projects/apps/herramientas/src/index.prod.html",
                "output": "index.html"
              },
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "4mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "20kb",
                  "maximumError": "20kb"
                }
              ]
            },
            "pre": {
              "fileReplacements": [
                {
                  "replace": "projects/apps/herramientas/src/environments/environment.ts",
                  "with": "projects/apps/herramientas/src/environments/environment.pre.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "4mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "20kb",
                  "maximumError": "20kb"
                }
              ]
            },
            "stage": {
              "fileReplacements": [
                {
                  "replace": "projects/apps/herramientas/src/environments/environment.ts",
                  "with": "projects/apps/herramientas/src/environments/environment.stage.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "4mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "20kb",
                  "maximumError": "20kb"
                }
              ]
            },
            "dev": {
              "fileReplacements": [
                {
                  "replace": "projects/apps/herramientas/src/environments/environment.ts",
                  "with": "projects/apps/herramientas/src/environments/environment.dev.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          },
          "defaultConfiguration": "prod"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "prod": {
              "browserTarget": "@apps/herramientas:build:prod"
            },
            "pre": {
              "browserTarget": "@apps/herramientas:build:pre"
            },
            "stage": {
              "browserTarget": "@apps/herramientas:build:stage"
            },
            "dev": {
              "browserTarget": "@apps/herramientas:build:dev"
            }
          },
          "defaultConfiguration": "dev"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "@apps/herramientas:build:dev"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/apps/herramientas/src/test.ts",
            "polyfills": "projects/apps/herramientas/src/polyfills.ts",
            "tsConfig": "projects/apps/herramientas/tsconfig.spec.json",
            "karmaConfig": "projects/apps/herramientas/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/apps/herramientas/src/favicon.ico",
              "projects/apps/herramientas/src/assets"
            ],
            "styles": [
              "projects/apps/herramientas/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
